.l-card{
	margin:30rpx;
	color:#fff;
	padding:35rpx;
	border-radius:15rpx;
	height:300rpx;
	display: flex;
	flex-direction: column;
	justify-content: space-between;
  background: #333 url(data:image/png;base64,) no-repeat right bottom;
  background-size:cover;
}
.l-userimg{
	width:80rpx;
	height:80rpx;
	overflow:hidden;
	border-radius:90px;
	margin-right:20rpx;
}
.l-userimg image{
	width:100%;
	height:100%;
}
.l-level{
	display: flex;
	font-size:26rpx;
}
.l-level>view{
	flex: 1;
  position: relative;
	text-align:center;
	font-size:24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.l-level>view::before{
  content: '';
  display: block;
  background: rgba(255, 255, 255, .5);
  width:14rpx;
  height:14rpx;
  border-radius:90px;
}
.l-level>view::last-child::before{
  background: transparent;
}
.l-level>view text{
  display: block;
  margin:0 20rpx;
  position: relative;
}

.l-level>view.active{
  color:#00cc99;
}
.l-level>view.active::before{
  background: #00cc99;
}
.l-col{
	display: flex;
	flex-wrap: wrap;
}
.l-col text{
	font-weight:600;
	color:#ff6655;
}
.l-col>view{
	line-height:2;
	width: 50%;
	font-size:26rpx;
	color:#888;
	display: flex;
	align-items: center;

}
.l-col>view::before{
	content: '';
	border-radius:90px;
	display: block;
	width:10rpx;
	height:10rpx;
	margin-right:10rpx;
	background: #ccc;
}
.l-col-block{
	display: block;
}
.l-col-block>view{
	width:inherit;
}
.l-tip{
	padding:20rpx 30rpx;
	font-size:24rpx;
}
.level-name{
	font-weight:600;
	color:#00cc99;
}
.l-item{
	padding:30rpx;
	background: #fff;
}
.l-title{
	font-weight:600;
	line-height: 1.6;
	font-size:26rpx;
	padding:20rpx 0 10rpx;
}
.l-item.status .level-name{
	color:#00cc99;
}
.level-name .ico{
	font-size:38rpx;
	margin-right:6rpx;
}
.level-title{
	text-align:center;
	font-weight: 600;
	padding:30rpx 0;
}

.l-block{
  margin-bottom:90rpx;
}
.l-shopname{
  font-size:24rpx;
}
.l-name{
  font-size:32rpx;
}