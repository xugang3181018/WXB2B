page {
  background: #fff;
}

.authentication {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.authentication image {
  width: 750rpx;
  height: 920rpx;
}

.authentication text {
  margin: 20rpx 0;
  font-size: 32rpx;
}

.authentication .help {
  color: #5177BE;
  margin-bottom: 35rpx;
  padding: 15rpx;
}

.authentication .finish {
  border: 1rpx solid #E5E5E5;
  padding: 10rpx 30rpx;
  border-radius: 8rpx;
}