.shoppingCart {
  /* height: 100vh; */
  /* background: #fff; */
}

.shoppingCart .labelNav {
  height: 80rpx;
  padding: 0 30rpx;
  font-size: 22rpx;
  color: #B19361;
  position: fixed;
  left: 0;
  width: 100%;
  box-sizing: border-box;
  background: #f6f3ed;
  z-index: 5;
}

.shoppingCart .labelNav .item .iconfont {
  font-size: 25rpx;
  margin-right: 10rpx;
}

.shoppingCart .nav {
  width: 100%;
  height: 99rpx;
  background-color: #fff;
  padding: 0 30rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #333333;
  position: fixed;
  left: 0;
  z-index: 5;
  border-bottom: 1px solid #eee;
}

.shoppingCart .nav .administrate {
  font-size: 24rpx;
  color: #333333;
  width: 140rpx;
  height: 50rpx;
  border-radius: 4rpx;
  border: 4rpx solid rgba(169,169,169,0.3);
}

.shoppingCart .noCart {
  margin-top: 171rpx;
  background: #f5f5f5;
  padding-top: 0.1rpx;
}

.shoppingCart .noCart .pictrue {
  width: 414rpx;
  height: 336rpx;
  margin: 78rpx auto 56rpx auto;
}

.shoppingCart .noCart .pictrue image {
  width: 100%;
  height: 100%;
}

.shoppingCart .list {
  margin-top: 171rpx;
}

.shoppingCart .list .title {
  padding: 0 30rpx;
  height: 84rpx;
  background: #fff;
  font-size: 30rpx;
  color: #333333;
  font-weight: bold;
  line-height: 84rpx;
  border-bottom: 1px solid #eee;
}

.shoppingCart .list .title checkbox {
  margin-right: 10rpx;
}

.shoppingCart .list .item {
  padding: 25rpx 30rpx;
  background-color: #fff;
  border-bottom: 1px solid #eee;
  /* margin-bottom: 15rpx; */
}

.shoppingCart .list .item .picTxt {
  width: 627rpx;
  position: relative;
}

.shoppingCart .list .item .picTxt .pictrue {
  width: 160rpx;
  height: 160rpx;
}

.shoppingCart .list .item .picTxt .pictrue image {
  width: 100%;
  height: 100%;
  border-radius: 6rpx;
}

.shoppingCart .list .item .picTxt .text {
  width: 444rpx;
  font-size: 24rpx;
  color: #333;
}

.shoppingCart .list .item .picTxt .text .infor {
  font-size: 20rpx;
  color: #999;
  margin-top: 16rpx;
  margin-bottom: 20rpx;
}

.shoppingCart .list .item .picTxt .text .order-hint {
  margin-left: 20rpx;
  color: #fc4141;
}

.shoppingCart .list .item .picTxt .text .money {
  font-size: 28rpx;
  color: #EB5533;
  /* margin-top: 28rpx; */
}

.shoppingCart .list .item .picTxt .carnum {
  height: 40rpx;
  position: absolute;
  bottom: 7rpx;
  right: 0;
}

.shoppingCart .list .item .picTxt .carnum view {
  /* border: 1rpx solid #a4a4a4; */
  /* width: 40rpx; */
  text-align: center;
  height: 100%;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #a4a4a4;
}

.shoppingCart .list .item .picTxt .carnum .reduce {
  font-size: 16rpx;
  font-weight: bold;
  color: #fff;
}

.shoppingCart .list .item .picTxt .carnum .reduce.on {
  border-color: #e3e3e3;
  color: #dedede;
}

.shoppingCart .list .item .picTxt .carnum .plus {
  color: #fff;
  line-height: 36rpx;
}

.shoppingCart .list .item .picTxt .carnum .num {
  width: 60rpx;
  color: #333;
  font-size: 28rpx;
}

.shoppingCart .list .item .picTxt .carnum .num input {
  z-index: 0;
}

.shoppingCart .list .item .picTxt .delete-btn {
  position: absolute;
  width:140rpx;
  height:50rpx;
  text-align: center;
  line-height:46rpx;
  right:0;
  bottom:10rpx;
  border: 4rpx solid rgba(235,85,51,0.3);
  border-radius: 2px;
  font-size: 12px;
  color: #EB5533;
}

.shoppingCart .invalidGoods {
  background-color: #fff;
}

.shoppingCart .invalidGoods .goodsNav {
  width: 100%;
  height: 66rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #282828;
}

.shoppingCart .invalidGoods .goodsNav .iconfont {
  color: #B19361;
  font-size: 28rpx;
  margin-right: 17rpx;
}

.shoppingCart .invalidGoods .goodsNav .del {
  font-size: 26rpx;
  color: #999;
}

.shoppingCart .invalidGoods .goodsNav .del .icon-shanchu1 {
  color: #999;
  font-size: 33rpx;
  vertical-align: -2rpx;
  margin-right: 8rpx;
}

.shoppingCart .invalidGoods .goodsList .item {
  padding: 20rpx 30rpx;
  border-top: 1rpx solid #f5f5f5;
}

.shoppingCart .invalidGoods .goodsList .item .invalid {
  font-size: 22rpx;
  color: #fff;
  width: 70rpx;
  height: 36rpx;
  background-color: #aaa;
  border-radius: 3rpx;
  text-align: center;
  line-height: 36rpx;
}

.shoppingCart .invalidGoods .goodsList .item .pictrue {
  width: 140rpx;
  height: 140rpx;
}

.shoppingCart .invalidGoods .goodsList .item .pictrue image {
  width: 100%;
  height: 100%;
  border-radius: 6rpx;
}

.shoppingCart .invalidGoods .goodsList .item .text {
  width: 433rpx;
  font-size: 28rpx;
  color: #999;
  height: 140rpx;
}

.shoppingCart .invalidGoods .goodsList .item .text .infor {
  font-size: 24rpx;
}

.shoppingCart .invalidGoods .goodsList .item .text .end {
  font-size: 26rpx;
  color: #bbb;
}

.shoppingCart .footer {
  width: 100%;
  height: 96rpx;
  background-color: #fff;
  position: fixed;
  bottom:0;
  /* padding: 0 30rpx; */
  padding-left: 30rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eee;
  box-shadow:-6rpx -6rpx 6rpx #eee;
  z-index:99999;
}

.shoppingCart .footer .checkAll {
  font-size: 28rpx;
  color: #282828;
  margin-left: 16rpx;
}

.shoppingCart .footer checkbox .wx-checkbox-input {
  background-color: #fafafa;
}

.shoppingCart .footer .money {
  font-size: 30rpx;
}

.shoppingCart .footer .money .money-symbol {
  font-size: 28rpx;
  color: #EB5533;
}

.shoppingCart .footer .money .money-number {
  font-size: 40rpx;
  color: #EB5533;
  margin-right:30rpx;
}

.shoppingCart .footer .placeOrder {
  background: #B19361;
  color: #fff;
  font-size:28rpx;
  width: 240rpx;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  /* margin-left: 22rpx; */
  z-index: 100;
}

.shoppingCart .footer .button .bnt {
  background: #B19361;
  color: #fff;
  font-size:28rpx;
  width: 240rpx;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  margin-left: 22rpx;
}

.shoppingCart .footer .button form~form {
  margin-left: 17rpx;
}


/* checkbox未选中钱样式 */
checkbox .wx-checkbox-input {
  width: 28rpx;
  height: 28rpx;
  border-radius: 50%;
  border: 1px solid#B19361;
}

/* checkbox选中后样式 */
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  width: 28rpx;
  height: 28rpx;
  border-radius: 50%;
  border: 1px solid#B19361;
  background: #fff !important;
  border-color: #B19361 !important;
}

/*checkbox选中后图标样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 18rpx;
  height: 18rpx;
  font-size: 0;
  background: #B19361;
  border-radius: 50%;
}