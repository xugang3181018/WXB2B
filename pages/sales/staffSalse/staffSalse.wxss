/* pages/staffSalse/staffSalse.wxss */

.s-head {
  background: #00cc99;
  color: #fff;
  padding: 0 30rpx;
}

.s-userimg {
  width: 110rpx;
  height: 110rpx;
  border-radius: 100px;
  overflow: hidden;
	margin-right:20rpx;
}

.s-userimg image {
  width: 110rpx;
  height: 110rpx;
}

.s-head-info {
  padding: 50rpx 0;
	font-size:26rpx;
}

.s-staff-name {
  margin-bottom: 10rpx;
	font-size:32rpx;
	font-weight:600;
}

.s-col {
  display: flex;
  align-items: center;
}

.s-block {
  background: #fff;
  margin: 30rpx;
  border-radius:10rpx;
}

.s-col>view {
  flex: 1;
  text-align: center;
  font-size: 24rpx;
  padding: 30rpx 0;
  color: #444;
}

.s-title {
  font-size: 28rpx;
  padding: 30rpx;
  font-weight: 600;
}
.s-card-title{
	font-size:28rpx;
	font-weight:600;
}
.s-card .arr{
	font-size:24rpx;
}
.s-card-title .ico{
	font-size:36rpx;
	margin-right:10rpx;
}
.s-col text {
  font-size: 40rpx;
  display: block;
	margin-bottom:8rpx;
	font-weight: 600;
  color: #00cc99;
}
.app text{
	font-size:52rpx;
	font-weight:normal;
  color: #00cc99;
}
.s-card {
  margin: 0 0;
  border-radius: 20rpx 20rpx 0 0;
	background: #333 url(data:image/png;base64,) no-repeat center bottom;
  background-size:cover;
  padding: 30rpx;
}
