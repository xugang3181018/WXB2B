@import "../../iconfont.wxss";
.m-list {
    background: #fff;
    padding: 0 55rpx;
}

.m-list>view, .m-list>navigator {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 30rpx;
    padding: 36rpx 0;
    color: #444;
    position: relative;
}

.m-list>view text, .m-list>navigator text {
    color: #888;
}

.m-list>view::before, .m-list>navigator::before {
    content: '';
    position: absolute;
    display: block;
    border-bottom: 1px solid #ddd;
    bottom: 0;
    height: 2px;
    left: -50%;
    right: -50%;
    background-origin: content-box;
    transform: scale(0.5);
    flex-grow: 1;
}

.m-list .ico {
    flex-basis: 46rpx;
    width: 50rpx;
    font-size: 46rpx;
    margin-right: 20rpx;
}

.m-name {
    flex-grow: 2;
}

.m-list>view:after, .m-list>navigator::after {
    content: '';
    display: block;
    width: 12rpx;
    height: 12rpx;
    border: 2px solid #eee;
    border-left: none;
    border-bottom: none;
    transform: rotate(45deg);
}

.m-list>view:last-child::after {
    border-bottom: none;
}

.m-list .exit:before {
    border: none;
}
.so-bar {
    padding: 0 40rpx;
    box-shadow: 0 4rpx 14px rgba(0, 0, 0, 0.1);
}

.sm-list {
    padding: 0;
}

.sm-list>view {
    padding: 34rpx 0;
    /* margin: 0 40rpx; */
}

.sm-list>view:last-child::before {
    border: none;
}

.sm-item>view {
    width: 30%;
}

.sm-item>view:first-child {
