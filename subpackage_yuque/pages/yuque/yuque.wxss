page {
  height: 100%;
}
.yuque .index {
  height: 42rpx;
  font-size: 42rpx;
  line-height: 42rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  background: #5177BE;
  padding: 30rpx;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
}
.yuque .list {
  margin: 0 30rpx;
  height: 99rpx;
  border-bottom: 1rpx solid #E5E5E5;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.yuque .list image {
  width: 40rpx;
  height: 40rpx;
}
.yuque .list view {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.yuque .list view image {
  margin-right: 28rpx;
}
.yuque .list view text {
  color: #888888;
  font-size: 28rpx;
  max-width: 500rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}