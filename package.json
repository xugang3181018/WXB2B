  "scripts": {
    "dev": "cross-env webpack-dev-server --open --hot --mode development",
    "build": "cross-env npm run format && npm run clean && webpack --mode production",
    "clean": "cross-env rimraf dist && mkdir dist",
    "test": "mocha --compilers js:babel-register --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test --watch",
    "lint": "npm run format && npm run fix && eslint src --ext .js --ext .jsx",
    "fix": "npm run format && eslint src --fix --ext .js --ext .jsx",
    "format": "prettier --write src/**/*.{js,jsx} ",
    "watch": "webpack --watch"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "prettier --tab-width 2 --write",
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": [
