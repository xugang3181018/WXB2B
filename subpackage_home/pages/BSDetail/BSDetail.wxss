page {
  height: 100%;
  background: #f7f7f7;
}
.BSDetail .businessBG {
  background: #5177BE;
}
.BSDetail .storeBG {
  background: #5177BE;
}
.BSDetail .storeColor {
  color: #5177BE;
}
.BSDetail > view:first-of-type {
  height: 240rpx;
  color: #fff;
  position: relative;
}
.BSDetail > view:first-of-type > text:first-of-type {
  font-size: 46rpx;
  max-width: 500rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  bottom: 77rpx;
  left: 43rpx;
  position: absolute;
}
.BSDetail > view:first-of-type > text:nth-of-type(2) {
  font-size: 24rpx;
  bottom: 31rpx;
  left: 43rpx;
  position: absolute;
}
.BSDetail > view:first-of-type .business {
  width: 156rpx;
  height: 95rpx;
  text-align: center;
  position: absolute;
  right: 36rpx;
  bottom: 37rpx;
}
.BSDetail > view:first-of-type .business view {
  width: 156rpx;
  height: 54rpx;
  background: #fff;
  border-radius: 27rpx;
  color: #5177BE;
  text-align: center;
  line-height: 54rpx;
  margin-bottom: 17rpx;
  position: absolute;
  top: 0;
}
.BSDetail > view:first-of-type .business text {
  display: inline-block;
  font-size: 24rpx;
  line-height: 24rpx;
  position: absolute;
  bottom: 0;
  left: 54rpx;
}
.BSDetail > view:first-of-type .store {
  position: absolute;
  background: #fff;
  border-radius: 21.5rpx;
  font-size: 24rpx;
  bottom: 64rpx;
  right: 40rpx;
  width: 105rpx;
  height: 43rpx;
  color: #5177BE;
  text-align: center;
  line-height: 43rpx;
}
.BSDetail .info {
  padding-left: 40rpx;
  background: #fff;
}
.BSDetail .info > view {
  padding-right: 40rpx;
  height: 88rpx;
  border-bottom: 1rpx solid #E5E5E5;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #333;
}
.BSDetail .info > view:last-of-type {
  border-bottom: 0;
}
.BSDetail .info > view text:first-of-type {
  color: #666;
}
.BSDetail .info > view view {
  display: flex;
  justify-content: center;
  align-items: center;
}
.BSDetail .info > view view image {
  width: 50rpx;
  height: 50rpx;
  margin-left: 22rpx;
}
.BSDetail .info > view view text {
  color: #333;
}
.BSDetail .clip {
  border-radius: 3rpx;
  width: 80rpx;
  height: 46rpx;
  margin-left: 12rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #5177BE;
  border: 1rpx solid #5177BE;
}
.BSDetail button {
  margin-top: 30rpx;
  font-size: 32rpx;
}
.BSDetail .canNotEdit {
  background: #E5E5E5;
  color: #fff;
  width: 670rpx;
  height: 84rpx;
  border-radius: 6rpx;
  line-height: 84rpx;
  margin: 30rpx auto;
  font-size: 32rpx;
  text-align: center;
}
.BSDetail .activationCode {
  max-width: 600rpx;
}