<!--银行列表-->
<view class="bankBranchList">
  <search-bar
    placeholder="请输入名称搜索"
    bind:onClear="clearSearch"
    bind:onConfirm="search"
  />
  <List pageNo="{{1}}" count="{{list.length}}" height="{{height}}" pageSize="{{null}}">
    <block wx:for="{{list}}" wx:key="index">
      <view class="item" bind:tap="selectBank" data-item="{{item}}">
        <text>{{item.bankName}}</text>
      </view>
    </block>
  </List>
</view>