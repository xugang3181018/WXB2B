{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "amd": true,
    "node": true
  },
  "globals": {
    "ENV": true
  },
  // "extends": ["eslint-config-recommended", "prettier"],
  "extends": [
    "plugin:prettier/recommended",
    "plugin:react-hooks/recommended",
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "rules": {
    "strict": 0,
    "no-console": 0,
    "parser": "babel-eslint",
    "no-unreachable": 0
  }
}