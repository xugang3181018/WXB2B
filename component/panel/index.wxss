.page-panel-mask {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  background: rgb(0, 0, 0, .5);
  animation: opacity .4s ease;
  z-index: 1;
  width: 100%;
  overflow-x: hidden;
}

.page-panel {
  width: 100%;
  overflow-x: hidden;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  z-index: 90;
  overflow: hidden;
  overflow-y: hidden;
}

.cart-clear {
  font-size: 24rpx;
  color: #9b9b9b;
  padding: 10rpx;
}

.cart-clear .ico {
  font-size: 28rpx;
  margin-right: 8rpx;
}

.page-panel-inner {
  /* max-height:1000rpx; */
  max-height: 1300rpx;
  background: #fff;
  position: absolute;
  left: 0;
  right: 0;
  bottom: -1px;
  z-index: 10;
  border-radius: 18rpx 18rpx 0 0;
  min-height: 600rpx;
  animation: sliderUp .5s ease;
  overflow-y: scroll;
}

.page-panel-inner.window {
  margin: 0 auto;
  bottom: inherit;
  width: 600rpx;
  height: 450rpx;
  top: 30%;
  border-radius: 20rpx;
  min-height: inherit;
  max-height: inherit;
}

.page-panel-title {
  padding: 20rpx 30rpx;
  font-size: 30rpx;
  font-weight: 600;
}

.close{
  position: absolute;
  right: 0;
  top:0;
  font-size:44rpx;
  padding:20rpx;
}