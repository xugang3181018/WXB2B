.m-head {
    background: #00cc99;
    display: flex;
    align-items: center;
    padding: 60rpx 40rpx;
}

.m-img {
    margin-right: 20rpx;
}

.m-img image {
    width: 90rpx;
    height: 90rpx;
    border-radius: 200rpx;
}

.m-info {
    color: #fff;
}

.m-list {
    background: #fff;
    padding: 0 55rpx;
}

.m-list>view, .m-list>navigator {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 30rpx;
    padding: 36rpx 0;
    color: #444;
    position: relative;
}

.m-list>view text, .m-list>navigator text {
    color: #888;
}

.m-list>view::before, .m-list>navigator::before {
    content: '';
    position: absolute;
    display: block;
    border-bottom: 1px solid #ddd;
    bottom: 0;
    height: 2px;
    left: -50%;
    right: -50%;
    background-origin: content-box;
    transform: scale(0.5);
    flex-grow: 1;
}

.m-list .ico {
    flex-basis: 46rpx;
    width: 50rpx;
    font-size: 46rpx;
    margin-right: 20rpx;
}

.m-name {
    flex-grow: 2;
}

.m-list>view:after, .m-list>navigator::after {
    content: '';
    display: block;
    width: 12rpx;
    height: 12rpx;
    border: 2px solid #eee;
    border-left: none;
    border-bottom: none;
    transform: rotate(45deg);
}

.m-list>view:last-child::after {
    border-bottom: none;
}

.m-list .exit:before {
    border: none;
}

.m-ts {
    font-size: 24rpx;
    margin-top: 10rpx;
}

