@keyframes ops {
    0% {
        opacity: 0.1;
    }
    100% {
        opacity: 0.8;
    }
}
.pay-cet{
    position:relative;
    display: flex;
    align-items:center;
    justify-content:space-between;
    background:#fff;
    padding:50rpx;
    margin:20rpx 0;
}
.vip-title{
    font-weight:800;
    color:#B79C60;
}
.vip-get{
    color:#B79C60;
    font-size:26rpx;
    font-weight:800;
}
.vip-txt{
    font-size:24rpx;
    color:#999;
}
.vip{
    position:absolute;
    left:-70rpx;
    top:-70rpx;
    border:70rpx solid transparent;
    border-bottom-color:#B79C60;
    transform:scale(.6) rotate(-45deg);
    
}
.vip::before{
    position:absolute;
    color:rgba(255, 255, 255, .9);
    top:20rpx;
    left:-20rpx;
}
.key{
    width: 33.3333333%;
    font-size:70rpx;
    font-weight:800;
    display: flex;align-items:center;
    justify-content:center;
    color:#555;
    transition:.3s;
}
.key.hidden{
    font-size:50rpx;
    color:#444;
}
.key.hidden::after{
    content:'';
    display:block;
    border:14rpx solid transparent;
    border-top-color:rgb(201, 47, 47);
}
.key-touch{
    background:#ccc;
    color:#333;
}
.total-empty{
    color:#999;
}
.price-line {
    width: 4rpx;
    height: 56rpx;
    background: #FF7F3A;
    animation: ops 1s ease-in-out infinite;
    margin-left: 8rpx;
}

.pay-head{
    display:flex;
    justify-content:space-between;
    padding:40rpx;
    background:#fff;
    align-items:center;
    font-size:28rpx;
}
.pay-total{
    font-weight:800;
    font-size:58rpx;
    color:#FF7F3A;
    position:relative;
    display: flex;
    align-items:center;
}
.pay-total text{
    font-size:44rpx;
    margin-right:10rpx;
}
.pay-total{
    font-size:28rpx;
}
.number{
    font-size:60rpx;
}
.total-empty .number{
    color:#ccc;
}
.key-warp{
    width: 75%;
    display: flex;
    align-items:center;
    justify-content:space-between;
    flex-wrap:wrap;
    text-align:center;
}
.key-button{
    width:25%;
    height:100%;
    position:absolute;
    right:0;
    bottom:0;
    top:0;
}
.key-button view{
    display:flex;
    align-items:center;
    justify-content:center;
}
.keyborad{
    background:#fff;
    display: flex;
    align-items:center;
    justify-content:space-between;
    flex-wrap:wrap;
    text-align:center;
    position:fixed;
    left:0;
    right:0;
    bottom:0;
}
.pay-name{
    font-size:26rpx;
    color:#444;
    padding:30rpx;
}
.pay-addcomment{
    padding:20rpx 40rpx;
    color:#666;
    font-size:28rpx;
}
.del{
    height:25%;
    font-size:50rpx;
    color:#555;
}
.pay-coupon{
    display:flex;
    align-items:center;
    justify-content:space-between;
    padding:0 40rpx 40rpx;
    color:#FF7F3A;
    font-size:26rpx;
    background:#fff;
}
.coupon-total{
    font-weight:800;
}
.pay{
    height:75%;
    background:#44B549;
    color:#fff;
}
.pay text{
    margin:0 50rpx;
    font-weight:800;
    font-size:40rpx;
}


@font-face {
    font-family: 'art';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}
[class^="ico-"], [class*=" ico-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'art' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.ico-coupon:before {
  content: "\e900";
}
.ico-del:before {
  content: "\e901";
}
.ico-keybord:before {
  content: "\e902";
}
.ico-ok:before {
  content: "\e903";
}
.ico-star:before {
  content: "\e904";
}
.ico-vip:before {
  content: "\e905";
}

.remark{
    position:fixed;
    left:0;
    right:0;
    top:0;
    bottom:0;
    background:rgba(0, 0, 0, .5);
}
.remark-cont{
    background:#fff;
    margin:20% 10%;
    padding:40rpx 40rpx 0;
    border-radius:8rpx;
}
.remark-cont input{
    font-size:26rpx;
    width:100%;
    padding:20rpx 0;
}
.remark-title{
    padding:10rpx 0;
    font-weight:800;
    font-size:28rpx;
    color:#444;
}
.remark-bar{
    display: flex;
    align-items:center;
    justify-content:space-between;
    flex-wrap:wrap;
}
.remark-bar>view{
    width:50%;
    text-align:center;
    padding:30rpx 0;
    font-size:30rpx;
    color:#555;
}
.remark-bar .yes{
    color:#44B549;
}