@import "../../iconfont.wxss";
.r {
    background: #fff;
    padding: 0 0;
}

.navbar {
    background: #fff;
    color: #444;
    display: flex;
    align-items: center;
    height: 45px;
    justify-content: center;
		position:relative;
}
.navbar>view.navbar-line{
	position: absolute;
	height:8rpx;
	bottom:0;
	left:11.6666666%;
	background:#00cc99;
	transition: .3s;
}
.navbar>view {
    text-align: center;
    font-size: 30rpx;
    line-height: 40px;
    box-sizing: border-box;
    position: relative;
    color:#444;
    opacity:.9;
    font-weight:800;
}
@keyframes line{
	0%{
		transform:scaleX(1)
	}
		100%{
		transform:scaleX(.6)
	}
	100%{
		transform:scaleX(1)
	}
}
.navbar>view.active {
    color: #00cc99;
    position: relative;
    z-index: 90;
    opacity:1;
}

.navbar>view.active::after {
    content: '';
    display: block;
    width: 50rpx;
    height: 8rpx;
    background: #00cc99;
    position: absolute;
    left: 50%;
    margin-left: -25rpx;
    bottom: -1rpx;
}

.r-head {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 0 30rpx;
    padding:40rpx 0;
    background:#fff;
}

.r-head>view {
    width: 50%;
    display: flex;
    align-items: center;
    font-size: 24rpx;
    padding: 30rpx;
}

.r-head>view:first-child {
    border-radius: 8rpx 0 0 8rpx;
}

.r-head>view:last-child {
    border-radius: 0 8rpx 8rpx 0rpx;
}

.r-head>view view {
    font-size: 24rpx;
}

.r-head .ico {
    font-size: 46rpx;
    margin-right: 20rpx;
}


.report-head {
    box-shadow: 0 1px 8px rgba(100, 100, 100, 0.1);
    background:#fff;
    position:relative;
    z-index: 2;
}

.r-head>view:first-child {
    background: #ff6655;
}

.r-head>view:last-child {
     background: #658ccf;
}

.r-head>view {
    height: 40px;
    color:#fff;
}

.r-head>view view view {
    font-size: 32rpx;
    font-weight: 800;
}




.r-grid {
